#!/bin/sh
# @configure_input@
export prefix=@prefix@
export LMON_REMOTE_LOGIN=@rshcmd@
export XPLAT_RSHCOMMAND=@rshcmd@
export XPLAT_RSH=@rshcmd@
if test -z $STAT_MRN_COMM_PATH; then
    export STAT_MRN_COMM_PATH=@MRNETCOMMNODEBIN@
fi
if test -z $STAT_LMON_LAUNCHMON_ENGINE_PATH; then
    export STAT_LMON_LAUNCHMON_ENGINE_PATH=@LAUNCHMONBIN@
fi
if test -z $MRNET_DEBUG_LOG_DIRECTORY; then
    export MRNET_DEBUG_LOG_DIRECTORY=/dev/null
fi
if test -z $STAT_CONNECT_TIMEOUT; then
    export STAT_CONNECT_TIMEOUT=120
fi
exec @prefix@/bin/STATbin -r 5 $@
