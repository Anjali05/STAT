#!/bin/sh
# @configure_input@
export prefix=@prefix@
export exec_prefix=@exec_prefix@
export PATH=$PATH:@DOTBINDIR@
export PYTHONPATH=@pythondir@:@pyexecdir@:@PYTHONPATH@

if test -n "$STAT_PREFIX" ; then
    export prefix=$STAT_PREFIX
    export PYTHONPATH=$STAT_PREFIX/lib/python2.6/site-packages/STAT:$STAT_PREFIX/lib64/python2.6/site-packages/STAT:@PYTHONPATH@
fi

exec @STATPYTHON@ @pythondir@/STATview.py $@
